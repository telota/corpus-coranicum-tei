              <title type="numeric" key="066:003:002-066:003:017">
                                    066:003:002-066:003:017
                                </title>
            </msItem>
          </msContents>
          <physDesc>
            <objectDesc>
              <supportDesc material="parchment">
                <extent>Folios: 36
                                    <dimensions unit="mm"><width>291</width><height>202</height></dimensions>
